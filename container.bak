docker run --name php -d -it -v  C:\Users\wjp13\projects\php\:/app --volumes-from store --link memcached jepson/php

docker run --name nginx -d -p 80:80 -v  C:\Users\wjp13\projects\nginx\:/app --volumes-from store --link php jepson/nginx
